@model SOSBilbao.Models.Supermercado

@{
    ViewData["Title"] = "Delete";
}




    <div class="eliminarFavorito">
        <h1 style="color:red">ELIMINAR SUPERMERCADO  </h1>

        <h3 style="font-style:italic">¿Estas seguro de querer eliminar este Supermercado?</h3>


        <hr />
        <dl class="row">
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a asp-action="Index">Volver a la lista de Supermercados</a>
        </form>
    </div>

